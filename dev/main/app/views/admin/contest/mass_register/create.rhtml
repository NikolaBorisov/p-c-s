<% if @csv_output -%>
    <%= h( @csv_output ) %> </br>
<% else -%>

 <%= form_tag( { :action => :create, :id => params[:id] }, { :multipart => true } )%>
    
    <fieldset style="width:200px">
    <legend>Select CSV file</legend>
    
        <label for="use rname_prefix">Username Prefix:</label> </br>
        <%= text_field_tag("username_prefix", "", :size => '6') %>
        <p> </p>
        
        <label for="csv_input_file_name">CSV File:</label> </br>
        <%= file_field_tag("csv_input_file_name") %>       
    
        
        <%= submit_tag("Register", {:class=>"submit"}) %>            
    
    </fieldset>
    
<%= end_form_tag %>

<% end -%>

<%= link_to('Back', :controller => "contest", :action => "edit", :id => params[:id] ) %>